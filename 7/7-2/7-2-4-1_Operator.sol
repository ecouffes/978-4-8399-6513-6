pragma solidity ^0.4.0;


contract Operator {
    function Operator(){
        // a = a + e
        a += e;
        // a = a - e
        a -= e;
        // a = a * e
        a *= e;
        // a = a / e
        a /= e;
        // a = a % e
        a %= e;
        // a = a | e
        a |= e;
        // a = a & e
        a &= e;
        // a = a ^ e
        a ^= e;
        // increment and decrement
        a++;
        a--;
    }
}
